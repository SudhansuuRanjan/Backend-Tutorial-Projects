   <%- include("header") -%> 

    <div class="box">
      <h1 id="heading"><%= listTitle %> </h1>
    </div>

    <div class="box">
      <% for( var i = 0 ; i < newLists.length ; i++){ %>
      <div class="item">
        <input type="checkbox" />
        <p><%= newLists[i] %></p>
      </div>
      <% } %>

      <form class="item" action="/" method="post">
      <input type="text" name="newItem" id=""  placeholder="New Item" autocomplete="off"/>
      <button type="submit" name="button" value="<%= listTitle %>">+</button>
    </form>
    </div>


    <%- include("footer") -%> 


 